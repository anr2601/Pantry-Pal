{
    "manifest_version": 3,
    "name": "Pantry Pal",
    "version": "1.0.0",
    "description": "Your one stop destination for checking out allergies!",
    "action": {
        "default_popup":"index.html"
    },
    "permissions": ["tabs", "identity", "activeTab", "storage"],
    "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
  }
  